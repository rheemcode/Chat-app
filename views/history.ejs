<%- include("partials/header") %>
<div class="row" style="padding-top:10px">
    <div class="col">
        <hr>
    </div>
    <div class="col-auto">
        <p class="h3" align="center">
            View Message History
        </p>
    </div>
    <div class="col">
        <hr>
    </div>
</div>
<div class="container">
    <div class="overflow-auto msgs" id="messages" style="overflow-y:scroll; height:500px;">
        <% if (messageHistory) { %>
        <% for(i = 0; i< messageHistory.length; i++) { %>
        <div class="container darker">
            <b style="color:#000" class="left">
                <%= messageHistory[i]["username"] %>
            </b>
            <p><%=messageHistory[i]["message"]%></p>
            <span class="time-left">
                <%= messageHistory[i]["time"] %>
            </span>
        </div>
        <%}%>
        <%}%>

    </div>
</div>

<%- include("partials/footer") %>